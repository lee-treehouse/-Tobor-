{
    "parser": "@typescript-eslint/parser",
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "extends": ["eslint:recommended", "prettier", "plugin:@typescript-eslint/recommended"],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "no-console": "off"
    }
}
